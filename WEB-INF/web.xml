<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd"> 
<web-app> 
	<display-name>jwf</display-name> 
	<servlet> 
		<servlet-name>FrontController</servlet-name> 
		<servlet-class>org.esgi.web.framework.core.FrontController</servlet-class> 
	</servlet> 
	<servlet-mapping> 
		<servlet-name>FrontController</servlet-name> 
		<url-pattern>/*</url-pattern> 
	</servlet-mapping> 
	
	<servlet>
		<servlet-name>index</servlet-name>
		<jsp-file>/work/index.jsp</jsp-file>
	</servlet>	 
	<servlet-mapping>
		<servlet-name>index</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>home</servlet-name>
		<jsp-file>/work/home.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>home</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>createCar</servlet-name>
		<jsp-file>/work/createCar.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>createCar</servlet-name>
		<url-pattern>/car/createCar</url-pattern>
	</servlet-mapping>
</web-app>
